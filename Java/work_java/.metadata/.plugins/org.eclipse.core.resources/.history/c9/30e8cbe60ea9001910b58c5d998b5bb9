package com.basic;

import java.util.Arrays;
import java.util.Scanner;

public class MethodTest4 {
	//1.메소드 정의 2.사용
	//max(): int형 매개변수 3개를 입력 받아 그 중 제일 큰 수를 리턴
	static int max(int a, int b, int c) {
		int tmp = (a>b)? a:b;
		return (tmp>c)? tmp:c;
	}
	//isExist(): int형 변수 한 개를 입력받아 그 값이 numbers 배열안에 있는지 체크한 후
	//그 위치값(index)을 리턴하는 메소드
	//만약 변수가 numbers 배열안에 없으면 -1을 리턴
	//또한 배열안에 있으면 "exist!", 없으면 "not exist!" 출력
	static int isExist(int num)
	
	public static void main(String[] args) {
		int[] data = {10,20,30,50,90,123,450,66,67};
		//작성된 메소드 사용
	}
}