package array2;

import java.util.Arrays;

public class Homogeneous_Practice {
	public static int n, r, casecount;
	public static int[] a;
	
	public static void homogeneous(int count) {
		if(count == r) {
			casecount++;
			System.out.println(Arrays.toString(a));
			return;
		}
	}
	
	public static void main(String[] args) {
		n=6;
		r=3;
		a=new int[3];
	}
}