package array1;
import java.util.Arrays;
import java.util.Random;
import java.util.Scanner;
import java.io.FileInputStream;

public class Solution_P1_Gravity_서울8반_정택진{
	public static void build_data(int[] data) {
		Random r = new Random();
		for(int i = 0; i < 100; i++) {
			data[i] = r.nextInt(100);
		}
	}
	public static void main(String args[]) throws Exception{
		int[] data = new int[100];
		int max_inter, pre_index, cur_inter, ap, dwi;
		
		for(int i = 0; i < 1; i++) {
			build_data(data);
			max_inter = -1;
			pre_index = -1;
			
			for(int j = 0; j < 100; j++) {
				cur_inter = -1;
				pre_index = -1;
				//처음 인덱스 검색
				for(int k = 0; k < data.length; k++) {
					if(pre_index == -1 && j == data[k]) {
						pre_index = j;
					}
				}
				//해당 숫자 첫 인덱스 다음부터 탐색
				if(pre_index != -1) {
					for(int k = pre_index + 1; k < data.length; k++) {
						if(j <= data[k]) {
							cur_inter = k - pre_index;
							pre_index = k;
						}
						
						if(cur_inter > max_inter) {
							max_inter = cur_inter;
							ap = pre_index;
							dwi = j;
						}
					}
				}
			}
			
			if(max_inter == -1)
				max_inter = data.length - pre_index;
			
			System.out.println(Arrays.toString(data));
			System.out.printf("#%d %d / %d / %d\n",i+1,max_inter,ap,dwi);
		}
	}
}